{{!< default}}

<main class="posts" role="main">

    <article class="post {{post_class}}">
        {{#post}}
        <header class="post-header">
            <h1 class="post-title"><a href="{{url}}">{{{title}}}</a></h1>
        </header>
         <section class="post-description">
                        {{content}}
            </section>
            {{/post}}
    </article>

</main>
